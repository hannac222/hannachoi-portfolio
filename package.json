{
  "name": "hannachoi-portfolio",
  "version": "1.0.0",
  "description": "Hanna Choi's Portfolio",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "deploy": "npm run export && cp ./now.json ./out && cd ./out && now --public",
    "dev": "next",
    "export": "rm -fr ./out && next build && next export && npm run minimizeOutFolder",
    "start": "next start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "minimizeOutFolder": "npm run uglify:main && rm ./out/_next/static/commons/main.js.map",
    "uglify:main": "uglifyjs ./out/_next/static/commons/main.js --output ./out/_next/static/commons/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hannac222/hannachoi-portfolio.git"
  },
  "author": "Hanna Choi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hannac222/hannachoi-portfolio/issues"
  },
  "homepage": "https://github.com/hannac222/hannachoi-portfolio#readme",
  "dependencies": {
    "@rematch/core": "^0.6.0",
    "@zeit/next-sass": "^1.0.1",
    "fullpage-react": "^3.0.31",
    "next": "^8.0.1",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-animated-css": "^1.0.4",
    "react-dom": "^16.8.2",
    "react-ga": "^2.5.7",
    "react-redux": "^5.0.7",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-plugin-inline-react-svg": "^1.0.1",
    "uglify-js": "^3.4.9"
  }
}
